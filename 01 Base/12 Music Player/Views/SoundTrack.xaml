<Page x:Class="_12_Music_Player.Views.SoundTrack"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_12_Music_Player.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SoundTrack">

    <Grid Background="DarkCyan">
        <Slider x:Name="slScale" TickFrequency="0.1" IsSnapToTickEnabled="True"/>
        <WrapPanel Margin="300,20,0,0">
            <Image  Height="30" Source="image/2.png"></Image>
            <Image  Height="40" Source="image/1.png"></Image>
            <Image  Height="30" Source="image/3.png"></Image>
        </WrapPanel>
    </Grid>
</Page>
